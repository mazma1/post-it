{
  "name": "post-it",
  "version": "1.0.0",
  "description": "A simple application that allows friends and colleagues create groups for messaging.",
  "main": "./app/client/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "postinstall": "webpack -p --config ./webpack.prod.config.js",
    "start": "npm run reset_db & npm run postinstall & babel-node -- app/server/bin/www",
    "build": "webpack --config webpack.prod.config.js -p --progress --colors",
    "start:dev": "nodemon --exec babel-node ./app/server/bin/www --ignore ./app/client",
    "create": "webpack",
    "create:dev": "webpack-dev-server --open",
    "babel-build": "babel --presets es2015 app/template/js/src -d app/template/js/dist",
    "run:test": "./node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill --timeout 10000 test/*.test.js",
    "test:server": "npm run rebuild:testdb && NODE_ENV=test PORT=4000 npm run run:test",
    "test:client": "jest __test__/  --verbose",
    "test": "NODE_ENV=test npm run test:server",
    "test-on-travis": "NODE_ENV=test npm run test && npm run coverage && npm run coveralls",
    "coverage": "NODE_ENV=test node node_modules/.bin/istanbul cover _mocha --compilers js:babel-register --require babel-polyfill test/*.test.js",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "reset_db": "NODE_ENV=production sequelize db:migrate:undo:all && NODE_ENV=production sequelize db:migrate:all"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mazma1/post-it.git"
  },
  "author": "Mary Mazi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mazma1/post-it/issues"
  },
  "homepage": "https://github.com/mazma1/post-it#readme",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-react-hmre": "^1.1.1",
    "bootstrap-css": "^4.0.0-alpha.5",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.1",
    "sinon": "^2.3.5",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "bootstrap-loader": "^2.1.0",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "config": "^1.26.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jsonwebtoken": "^7.4.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "materialize-css": "^0.99.0",
    "materialize-loader": "^2.0.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "nexmo": "^2.0.2",
    "nodemailer": "^4.0.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "pg": "^6.2.4",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-modal": "^2.2.2",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^3.23.0",
    "sequelize-cli": "^2.7.0",
    "shortid": "^2.2.8",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "uuid": "^3.0.1",
    "validator": "^7.0.0",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.18.0"
  }
}
