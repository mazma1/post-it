{
  "name": "post-it",
  "version": "1.0.0",
  "description": "A simple application that allows friends and colleagues create groups for messaging.",
  "main": "./app/client/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node app/server/bin/www",
    "start:dev": "nodemon ./app/server/bin/www",
    "create": "webpack",
    "create:dev": "webpack-dev-server --open",
    "build": "babel --presets es2015 app/template/js/src -d app/template/js/dist",
    "test": "./node_modules/.bin/mocha tests/*.js",
    "coverage": "node node_modules/.bin/istanbul cover _mocha tests/*tests.js",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "reset_db": "NODE_ENV=production sequelize db:migrate:undo:all && NODE_ENV=production sequelize db:migrate:all"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mazma1/post-it.git"
  },
  "author": "Mary Mazi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mazma1/post-it/issues"
  },
  "homepage": "https://github.com/mazma1/post-it#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap-css": "^4.0.0-alpha.5",
    "bootstrap-sass": "^3.3.7",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.29.0",
    "istanbul": "^0.4.5",
    "materialize-sass": "^0.98.2",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.5",
    "style-loader": "^0.18.2",
    "supertest": "^3.0.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "bootstrap-css": "^4.0.0-alpha.5",
    "bootstrap-loader": "^2.1.0",
    "config": "^1.26.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "file-loader": "^0.11.2",
    "jsonwebtoken": "^7.4.1",
    "jwt-simple": "^0.5.1",
    "materialize-css": "^0.99.0",
    "materialize-loader": "^2.0.0",
    "morgan": "^1.8.2",
    "pg": "^6.2.4",
    "pg-hstore": "^2.3.2",
    "react": "^15.6.1",
